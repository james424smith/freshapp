{
  "name": "marlowCrewCompanion",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "start:ios": "yarn rnuc .env.staging && react-native run-ios",
    "start:ios:prod": "yarn rnuc .env.production && react-native run-ios",
    "start:android": "yarn rnuc .env.staging && react-native run-android",
    "start:android:prod": "yarn rnuc .env.production && react-native run-android",
    "pod:install": "yarn pod-install ios",
    "build:ios": "yarn rnuc .env && react-native bundle --minify --entry-file index.js --bundle-output ios/main.jsbundle --assets-dest ios/assets --dev false --platform ios",
    "build:android": "yarn rnuc .env && react-native bundle --minify --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res --dev false --platform android",
    "clean:android": "cd android && ./gradlew clean && cd ..",
    "release:android:staging": "cd android && yarn rnuc .env.staging && ./gradlew assembleBetaStagingrelease && cd ..",
    "release:android:prod": "cd android && yarn rnuc .env.production && ./gradlew assembleProdRelease && cd ..",
    "test": "jest --runInBand --forceExit --detectOpenHandles && jest-coverage-badges",
    "test:update": "jest --runInBand --forceExit --detectOpenHandles -u",
    "test:sonar": "jest --runInBand --forceExit --detectOpenHandles && jest-coverage-badges && node analyze.js",
    "test:ci": "cross-env CI=true jest --runInBand --forceExit --detectOpenHandles && jest-coverage-badges",
    "prettify": "node_modules/.bin/prettier --config /.prettierrc --write \"**/*.ts*\" --write \"**/*.js\" --write \"**/*.json\"",
    "tsc": "tsc --p ./tsconfig.json",
    "lint": "eslint --config .eslintrc.js --ext .ts --ext .tsx --ext .js .",
    "test-compile": "yarn lint && yarn tsc && yarn test",
    "postinstall": "yarn jetify"
  },
